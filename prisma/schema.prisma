datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  address   String
  phone     String
  rg        String
  birthDate DateTime
  role      Role     @default(USER)
}

enum Role {
  ADMIN
  USER
}
